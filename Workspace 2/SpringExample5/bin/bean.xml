<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   
   <bean id="helloworld" class="com.devansh.HelloWorld">
   <property name="message1" value="Hi Sexxxxy"/>
   <property name="message2" value="Again Hi Sexxxxy"/>
   </bean>
   
   <bean id="helloindia" class="com.devansh.HelloIndia" parent="helloworld">
   <property name="message1" value="Hello India"/>
   <property name="message3" value="Namasteeeee India"></property>
   </bean>
   </beans>